 <%= render partial: "components/navbar"%>
 <section>
<ul class="list-unstyled">
    <% @users.each do |user| %>
        <li class="user_list">
           
                <div class="user_prof">
                <img src=<%= user.photo %> alt="profile pic">
                </div>
                <div class="user_data">
                <h2><%= link_to user.name, user_path(user.id) %></h2>

                    <p>Post number: <%= user.posts_count %></p>
                </div>
        </li>
    <% end %>
</ul>
</section>
